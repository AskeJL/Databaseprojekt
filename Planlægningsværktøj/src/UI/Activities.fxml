<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/10.0.1" fx:controller="UI.ActivitiesController">
   <children>
      <Label fx:id="dayLabel" layoutX="169.0" layoutY="6.0" text="Default day">
         <font>
            <Font name="Comic Sans MS" size="48.0" />
         </font>
      </Label>
      <ListView fx:id="activitiesLv" layoutX="14.0" layoutY="97.0" prefHeight="291.0" prefWidth="292.0" />
      <ImageView fx:id="pictogramIv" fitHeight="152.0" fitWidth="263.0" layoutX="318.0" layoutY="97.0" pickOnBounds="true" preserveRatio="true" />
      <TextArea fx:id="pictogramTa" editable="false" focusTraversable="false" layoutX="318.0" layoutY="249.0" mouseTransparent="true" prefHeight="104.0" prefWidth="263.0" />
      <Button fx:id="removeActivityBtn" layoutX="475.0" layoutY="361.0" mnemonicParsing="false" onAction="#removeActivityBtnHandle" prefHeight="27.0" prefWidth="106.0" text="Slet aktivitet" />
      <Button fx:id="createActivityBtn" layoutX="318.0" layoutY="361.0" mnemonicParsing="false" onAction="#createActivityBtnHandle" text="Opret aktivitet" />
   </children>
</AnchorPane>
